<button class="btn btn-back">          
  <%= link_to subject_classes_path(subject_id: @subject_class.subject_id) do %>
    <i class="fas fa-arrow-left"></i>
  <% end %>
</button>

<h2>New Teacher</h2>

<%= form_for(@subject_class_teacher, url: subject_class_teachers_path, method: :post) do |f| %>

  <%= render partial: "form", locals: { f: f } %>

  <div class="actions">
    <%= f.submit "Create Teacher" %>
  </div>
<% end %>

