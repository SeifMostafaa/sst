<button class="btn btn-back">          
  <%= link_to time_tables_path(subject_class_id: @time_table.subject_class_id) do %>
    <i class="fas fa-arrow-left"></i>
  <% end %>
</button>



<div class="row justify-content-md-center">
    <div class="col-md-6 col-lg-12 border shadow-sm rounded p-3 mt-5 mb-3">

    <h1>Edit Time Table</h1>

    <%= form_for(@time_table, url: time_table_path(id: @time_table.id), method: :put) do |f| %>

    <%= render partial: "form", locals: { f: f } %>

 <br/>
  <div class="form-group text-center">
    <%= f.submit "Update Time Table", class: "btn btn-primary" %>
  </div>
   <% end %>
  </div>
</div>


