<button class="btn btn-back">          
  <%= link_to time_tables_path(subject_class_id: @subject_class.id) do %>
    <i class="fas fa-arrow-left"></i>
  <% end %>
</button>


<div class="row justify-content-md-center">
    <div class="col-md-6 col-lg-12 border shadow-sm rounded p-3 mt-5 mb-3">

    <h1>New Time Table</h1>

<%= form_for(@time_table, url: time_tables_path(subject_class_id: @subject_class.id), method: :post) do |f| %>

  <%= render partial: "form", locals: { f: f } %>

 <br/>
  <div class="form-group text-center">
    <%= f.submit "Create Time Table", class: "btn btn-primary" %>
  </div>
   <% end %>
  </div>
</div>


